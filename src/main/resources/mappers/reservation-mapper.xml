<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
  <mapper namespace="ReservationMapper">
	<resultMap type="Reservation" id="reservationResultMap">
		<id 	property="reservationNo" 		column="RESERVATION_NO" />
		<result property="memberId" 		column="MEMBER_ID"/>
		<result property="checkIn" 		column="CHECK_IN"/>
		<result property="checkOut" 		column="CHECK_OUT"/>
		<result property="rStatus" 		column="R_STATUS"/>
		<result property="roomNo" 		column="ROOM_NO"/>
		<result property="pensionNo" 		column="PENSION_NO"/>
		<result property="useHistory" 		column="USE_HISTORY"/>
		<result property="price" 		column="PRICE"/>
	</resultMap>
	
